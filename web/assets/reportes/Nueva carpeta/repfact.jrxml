		<parameter name="coddet" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT businessmary.orderdetails.quantity,
	businessmary.orderdetails.discount,
	businessmary.orderdetails.amount,
	SUBSTRING(businessmary.orderdetails.productname,1,22) as namep,
	businessmary.orderdetails.headerid,
	ROUND(businessmary.orderdetails.peso,0) as peso,
	businessmary.orderdetails.simbolo,
	ROUND(businessmary.orderdetails.amount * businessmary.orderdetails.quantity,2) AS importe
