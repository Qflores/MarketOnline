	<queryString>
		<![CDATA[SELECT businessmary.headerpurchaseorder.id,
	businessmary.headerpurchaseorder.purchasecode,
	businessmary.headerpurchaseorder.orderdate,
	businessmary.headerpurchaseorder.igv,
	businessmary.headerpurchaseorder.totalamount,
	businessmary.headerpurchaseorder.subtotal,
	businessmary.person.name,
	businessmary.person.firstname,
	businessmary.person.lastname,
	businessmary.person.numdoc,
	businessmary.contact.adress
FROM businessmary.headerpurchaseorder
	INNER JOIN businessmary.customer ON 
	 businessmary.headerpurchaseorder.customer_id = businessmary.customer.id 
	INNER JOIN businessmary.person ON 
	 businessmary.customer.person_Id = businessmary.person.Id
	INNER JOIN businessmary.contact ON 
	 businessmary.contact.person_Id = businessmary.person.Id
WHERE 
	 businessmary.headerpurchaseorder.purchasecode = $P{codfact}]]>
	</queryString>
